	}
	public void setOrderDetailProperty(OrderDetail orderDetail){
		this.orderID_s = orderDetail.getOrderID_s();
		this.orderName_s = orderDetail.getOrderName_s();
		this.reservationDate_s = DateUtil.dateToString(orderDetail.getReservationDate_t(),"yyyy-MM-dd HH:mm:ss");
		this.status_i = orderDetail.getStatus_i();
		this.customerID_s = orderDetail.getCustomerID_s();
		this.startDate_s = DateUtil.dateToString(orderDetail.getStartDate_t(),"yyyy-MM-dd HH:mm:ss");
//System.out.println("startDate_s="+startDate_s);
		this.endDate_s = DateUtil.dateToString(orderDetail.getEndDate_t(),"yyyy-MM-dd HH:mm:ss");
