		this.employeeID_s = orderDetail.getEmployeeID_s();
		this.invoiceOrder_s = orderDetail.getInvoiceOrder_s();
		this.addedValueTax_s = orderDetail.getAddedValueTax_s();
		this.invoiceUser_s = orderDetail.getInvoiceUser_s();
		this.receiveInvoiceAddress_s = orderDetail.getReceiveInvoiceAddress_s();
		this.invoiceUserPhone_s = orderDetail.getInvoiceUserPhone_s();
		this.customerAppraise_s = orderDetail.getCustomerAppraise_s(); 
	}
	public void setCustomerProperty(Customer customer){
