		<property name="createUser_s" column="createUser_s" />
		<property name="lastUpdateUser_s" column="lastUpdateUser_s" />
		<property name="lastUpdateDate_t" column="lastUpdateDate_t" />		
		<property name="resaveds1_s" column="resaveds1_s" />
