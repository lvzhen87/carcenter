package com.freshen.util;

public class PageMsgBox {

	public static String showMsgBox(String msg){
		StringBuffer sb=new StringBuffer(); 
		sb.append("<script type=\"text/javascript\"> alert('");
		sb.append(msg);
		sb.append("')</script>");
		return sb.toString();
	}
	public static void main(String[] args) {
		//System.out.println(PageMsgBox.showMsgBox("ddd"));
	}
}
